Algoritmo alg11SEC
	Definir X1,X2,D Como Real;
	Escribir "Programa que indica la distancia entre dos numeros";
	Escribir "Ingrese el número menor";
	Leer X1;
	Escribir "Ingrese el número mayor";
	Leer X2;
	
	D<-X2-X1;
	
	Escribir "La distancia entre ambos numeros es de : " D;
	
FinAlgoritmo
