Algoritmo SEC12
	Definir x1,y1,x2,y2,A,B,D Como Real;
	Escribir "Ingrese x1";
	Leer x1;
	Escribir "Ingrese y1";
	Leer y1;
	Escribir "Ingrese x2";
	Leer x2;
	Escribir "Ingrese y2";
	Leer y2;
	
	A<-x1+y1;
	B<-x2+y2;
	D<-abs(A-B);
	
	Escribir " La distancia es de : " D;
	
FinAlgoritmo
